{
  "name": "flight-check-in-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test:experimental": "ng test",
    "test": "jest",
    "test:e2e": "npx cypress open",
    "gql:update-schema": "graphql-codegen get-graphql-schema --url=http://localhost:4000/graphql --output=./src/app/modules/check-in/graphql/booking.schema.graphql --config=./src/app/modules/check-in/graphql/booking.codegen.yml",
