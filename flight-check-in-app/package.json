    "i18n:extract": "ng extract-i18n --output-path src/locale",
    "env:generate": "ng generate environments",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "push:skip-ci": "git push -o ci.skip"
